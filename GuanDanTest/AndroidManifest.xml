<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="guandan.game" android:versionCode="1" android:versionName="1.0">
	<application android:icon="@drawable/icon">
		<activity android:name=".MenuActivity" android:label="@string/app_name"
			android:theme="@android:style/Theme.NoTitleBar"
			android:screenOrientation="landscape">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<activity android:name=".HelpActivity" android:label="@string/help_title"
			android:theme="@android:style/Theme.Dialog">
			<intent-filter>
				<action android:name="android.intent.action.VIEW"></action>
				<category android:name="android.intent.category.DEFAULT"></category>
			</intent-filter>
		</activity>

		<activity android:name=".GameActivity" android:theme="@android:style/Theme.NoTitleBar"
			android:screenOrientation="landscape">
			<intent-filter>
				<action android:name="android.intent.action.VIEW"></action>
				<category android:name="android.intent.category.DEFAULT"></category>
			</intent-filter>
		</activity>
		
		<activity android:name=".SettingActivity" android:theme="@android:style/Theme.NoTitleBar"
			android:screenOrientation="landscape">
			<intent-filter>
				<action android:name="android.intent.action.VIEW"></action>
				<category android:name="android.intent.category.DEFAULT"></category>
			</intent-filter>
		</activity>

	</application>


</manifest> 